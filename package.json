{
  "name": "haversack",
  "title": "Haversack",
  "description": "Easy save states through browser LocalStorage or SessionStorage.",
  "author": "Colin Hemphill <colin@colinhemphill.com>",
  "homepage": "https://github.com/colinhemphill/haversack",
  "license": "MIT",
  "version": "1.2.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinhemphill/haversack.git"
  },
  "bugs": {
    "url": "https://github.com/colinhemphill/haversack/issues"
  },
  "keywords": [
    "haversack",
    "save",
    "state",
    "localStorage",
    "sessionStorage",
    "react",
    "hooks"
  ],
  "scripts": {
    "build": "tsup --dts",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "prettier --write 'src/**/*.{json,ts,tsx}'",
    "lint": "eslint ./src --ext js --ext ts --ext tsx --fix",
    "prepare": "npm run build && husky install",
    "test": "jest --coverage",
    "types": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "8.0.0",
    "@types/jest": "28.1.1",
    "@types/react": "18.0.12",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-localstorage-mock": "2.4.21",
    "lint-staged": "13.0.1",
    "mockdate": "3.0.5",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "react-test-renderer": "18.1.0",
    "ts-jest": "28.0.4",
    "tsup": "6.1.2",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
