{
  "name": "haversack",
  "title": "Haversack",
  "description": "Easy save states through browser LocalStorage or SessionStorage.",
  "author": "Colin Hemphill <colin@colinhemphill.com>",
  "homepage": "https://github.com/colinhemphill/haversack",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinhemphill/haversack.git"
  },
  "bugs": {
    "url": "https://github.com/colinhemphill/haversack/issues"
  },
  "keywords": [
    "haversack",
    "save",
    "state",
    "localStorage",
    "sessionStorage",
    "react",
    "hooks"
  ],
  "scripts": {
    "build": "tsc",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/react": "16.9.52",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "react": "16.14.0",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "flatted": "3.1.0"
  }
}
