{
  "name": "haversack",
  "title": "Haversack",
  "description": "Easy save states through browser LocalStorage or SessionStorage.",
  "author": "Colin Hemphill <colin@colinhemphill.com>",
  "homepage": "https://github.com/colinhemphill/haversack",
  "license": "MIT",
  "version": "1.3.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinhemphill/haversack.git"
  },
  "bugs": {
    "url": "https://github.com/colinhemphill/haversack/issues"
  },
  "keywords": [
    "haversack",
    "save",
    "state",
    "localStorage",
    "sessionStorage",
    "react",
    "hooks"
  ],
  "scripts": {
    "build": "tsup --dts",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "prettier --write 'src/**/*.{json,ts,tsx}'",
    "lint": "eslint ./src --ext js --ext ts --ext tsx --fix",
    "prepare": "npm run build && husky install",
    "test": "jest --coverage",
    "types": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@swc/core": "1.3.83",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "29.5.4",
    "@types/react": "17.0.65",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "jest-localstorage-mock": "2.4.26",
    "lint-staged": "14.0.1",
    "mockdate": "3.0.5",
    "prettier": "3.0.3",
    "react": "17.0.2",
    "react-test-renderer": "17.0.2",
    "ts-jest": "29.1.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}