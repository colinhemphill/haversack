{
  "name": "haversack",
  "title": "Haversack",
  "description": "Easy save states through browser LocalStorage or SessionStorage.",
  "author": "Colin Hemphill <colin@colinhemphill.com>",
  "homepage": "https://github.com/colinhemphill/haversack",
  "license": "MIT",
  "version": "1.2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinhemphill/haversack.git"
  },
  "bugs": {
    "url": "https://github.com/colinhemphill/haversack/issues"
  },
  "keywords": [
    "haversack",
    "save",
    "state",
    "localStorage",
    "sessionStorage",
    "react",
    "hooks"
  ],
  "scripts": {
    "build": "tsc",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "prettier --write 'src/**/*.{json,ts,tsx}'",
    "lint": "eslint ./src --ext js --ext ts --ext tsx --fix",
    "prepare": "npm run build && husky install",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.1",
    "@types/react": "17.0.19",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "jest": "27.1.0",
    "jest-localstorage-mock": "2.4.17",
    "lint-staged": "11.1.2",
    "mockdate": "3.0.5",
    "prettier": "2.4.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17"
  }
}