{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "ignorePatterns": [
    ".dist/*",
    "node_modules/*"
  ],
  "rules": {
    "sort-imports": "off",
    "sort-keys": "warn",
    "sort-vars": "warn"
  }
}